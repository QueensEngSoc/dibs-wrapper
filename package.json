  "scripts": {
    "heroku-prebuild": "npm install --dev && touch config/emailConfig.js && npm run build",
    "test": "NODE_ENV=mocha mocha 'test/**/*.ts' 'test/*.ts' 'test/**/*.js' 'test/*.js'",
    "debug": "NODE_ENV=dev npm run build && concurrently -n watch,debug \"npm:watch:*\" \"npm:debug:server\"",
    "debug:server": "NODE_ENV=dev nodemon --watch dist ./dist/server.js",
    "clean": "rm -rf ./dist ./public/*.js ./public/*.map",
    "pretest": "npm run tsc && NODE_ENV=dev npm run build:client",
    "tsc": "tsc --noEmit",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "webpack --progress --colors --config ./config/webpack.config.client.js",
    "build:server": "webpack --progress --colors --config ./config/webpack.config.server.js",
    "watch": "concurrently -n watch \"npm:watch:client\" \"npm:watch:server\"",
    "watch:server": "webpack --watch --progress --colors --config ./config/webpack.config.server.js",
    "watch:client": "webpack --watch --progress --colors --config ./config/webpack.config.client.js",
    "start": "node ./dist/server.js",
    "debug:windows": "NODE_ENV=dev npm run build:windows && concurrently -n watch,debug \"npm:watch:*\" \"npm:debug:server\"",
    "build:windows": "npm run build:clientwindows && npm run build:server",
    "build:clientwindows": "set BABEL_ENV=client webpack --progress --colors --config ./config/webpack.config.client.js",
    "clean:windows": "rmdir /s /q dist public/*.js public/*.map"
  },
